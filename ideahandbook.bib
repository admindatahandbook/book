@article{dhaliwal2017,
  title = {The Devil Is in the Details: {{The}} Successes and Limitations of Bureaucratic Reform in {{India}}},
  shorttitle = {The Devil Is in the Details},
  author = {Dhaliwal, Iqbal and Hanna, Rema},
  year = {2017},
  month = jan,
